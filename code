Sub stock_data()

Range("J1") = "Ticker"
Range("K1") = "Yearly Change"
Range("L1") = "Percent Change"
Range("M1") = "Total Stock Volume"

Dim Ticker As String
Dim Yearly_Change As Double
Dim Percent_Change As Double
Dim Total_Stock_Volume As Long

Dim Summary_Table_Row As String
Summary_Table_Row = 2

For i = 2 To 255

For j = 1 To 1
    'if the value in the row below the row you are looping through is not equal to the data in the row you are in then return that ticker symbol in defined range
    If Cells(i + 1, 1).Value <> Cells(i, 1).Value Then
    Ticker = Cells(i, 1).Value

    Range("J2").Value = Ticker
    
    Else
    
    Summary_Table_Row = Summary_Table_Row + 1
    
    
    End If
    
    Next j
    
    Next i


End Sub
